{
    "name": "k-syntax",
    "displayName": "k-syntax",
    "description": "k syntax demo",
    "version": "0.0.1",
    "icon": "k-logo.png",
    "author": {
        "name": "Pumpk1n"
    },
    "publisher": "Pumpk1n",
    "repository": {
        "url": "https://github.com/PumpkinDemo/k-syntax-vscode"
    },
    "engines": {
        "vscode": "^1.71.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "kframework",
                "aliases": ["k", "kframework"],
                "extensions": [".k"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kframework",
                "scopeName": "source.k",
                "path": "./syntaxes/kframework.tmLanguage.json"
            }
        ]
    }
}